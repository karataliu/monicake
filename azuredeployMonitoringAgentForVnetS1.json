{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string",
      "metadata": {
        "description": "vnet name."
      },
      "defaultValue": "myvnet"
    },
    "subnetName": {
      "type": "string",
      "metadata": {
        "description": "subnet name."
      },
      "defaultValue": "Subnet"
    }
  },
  "resources": [],
 "outputs": {
    "data": { 
        "value": "[reference(concat('Microsoft.Network/virtualnetworks/', parameters('vnetName'), '/subnets/', parameters('subnetName')), '2016-03-30').ipConfigurations]",
        "type" : "array"
    }
  }
}

